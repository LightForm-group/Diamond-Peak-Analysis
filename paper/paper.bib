keywords = {Deformation mechanism,Hot deformation,Load partition,Plasticity,Synchrotron x-ray diffraction,Titanium alloys},
mendeley-groups = {Zr {\&} Zr-Nb Texture/Alpha + Beta Deformation Ti/Dual Phase Deformation,Reference Sources/Peter Honniball Lit Review},
month = {mar},
pages = {244--258},
publisher = {Elsevier},
title = {{Load partition and microstructural evolution during in situ hot deformation of Ti-6Al-6V-2Sn alloys}},
url = {http://dx.doi.org/10.1016/j.msea.2016.01.059 http://linkinghub.elsevier.com/retrieve/pii/S0921509316300600},
