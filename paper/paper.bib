doi = {10.1107/S1600577515002283},
file = {:Users/mbcx9cd4/Downloads/References/fv5032.pdf:pdf},
issn = {16005775},
journal = {Journal of Synchrotron Radiation},
keywords = {DAWN,analysis Special issue on X-ray Free-Electron Lasers,software,visualisation},
mendeley-groups = {LightForm/Synchrotron/DAWN},
pages = {853--858},
title = {{Data Analysis WorkbeNch (DAWN)}},
volume = {22},
year = {2015}
}

